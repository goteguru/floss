  pages = {20-25},
  keywords = {security,Atomic measurements,Energy measurement,Hardware,Internet of Things,IoT,IoT device security,Monitoring,energy efficiency,energy efficient security,first-order security consideration,goals metrics,measurement frameworks,power aware computing,power measurement,power measurement methodologies,security of data},
  author = {Ott, D. E.},
