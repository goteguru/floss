  keywords = {nyílt forrás,vulnerability,Open Source,code review,inspection,security defects,_SMS_untagged},
  author = {Bosu, Amiangshu and Carver, Jeffrey C. and Hafiz, Munawar and Hilley, Patrick and Janni, Derek},
